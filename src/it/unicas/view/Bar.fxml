<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="it.unicas.view.BarOverviewController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <Label alignment="TOP_LEFT" text="Ordine:" textFill="#5b5757" BorderPane.alignment="CENTER">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
            </top>
            <center>
                <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <columns>
                        <TableColumn prefWidth="75.0" text="Nome" />
                        <TableColumn prefWidth="75.0" text="Quantita" />
                        <TableColumn prefWidth="75.0" text="Preparato" />
                    </columns>
                </TableView>
            </center>
            <bottom>
                <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <buttons>
                        <Button disable="true" mnemonicParsing="false" text="Preparato">
                            <font>
                                <Font size="15.0" />
                            </font></Button>
                    </buttons>
                </ButtonBar>
            </bottom>
        </BorderPane>
    </children>

</AnchorPane>
